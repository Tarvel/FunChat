{
  "version": 2,
  "builds": [
    {
      "src": "funchat/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "hook": "python manage.py collectstatic --noinput"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "funchat/wsgi.py"
    }
  ]
}
